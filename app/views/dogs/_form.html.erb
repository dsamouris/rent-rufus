
<%= simple_form_for @dog do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :activity %>
  <%= f.input :breed %>
  <%= f.input :age %>
  <%= f.input :size, collection: ["Small", "Medium", "Large"] %>
  <%= f.button :submit %>
<% end %>

